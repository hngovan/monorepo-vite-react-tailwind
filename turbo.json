{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "dev": {
      "dependsOn": [
        "^dev"
      ],
      "persistent": false,
      "cache": false
    },
    "clean": {
      "cache": false
    },
    "build": {
      "dependsOn": [
        "^build"
      ],
      "inputs": [
        "$TURBO_DEFAULT$",
        ".env*"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "auth:schema:generate": {
      "cache": false,
      "interactive": true
    },
    "format": {
      "outputs": [
        ".cache/.prettiercache"
      ],
      "outputLogs": "new-only"
    },
    "lint": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        ".cache/.eslintcache"
      ]
    },
    "typecheck": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        ".cache/tsbuildinfo.json"
      ]
    },
    "start": {
      "dependsOn": [
        "^build"
      ]
    },
    "studio": {
      "cache": false,
      "persistent": true
    },
    "ui-add": {
      "cache": false,
      "interactive": true
    },
    "env:copy-example": {
      "cache": false
    },
    "env:remove": {
      "cache": false
    }
  },
  "globalDependencies": [
    "**/.env.*local"
  ],
  "globalPassThroughEnv": [
    "NODE_ENV",
    "CI",
    "npm_lifecycle_event"
  ]
}